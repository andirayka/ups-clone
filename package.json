{
  "name": "ups_clone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android && react-native start",
    "android_resetcache": "react-native run-android && react-native start --reset-cache",
    "android_release": "react-native run-android --variant=release",
    "start_resetcache": "yarn start --reset-cache",
    "start_adb": "yarn reconnect_usbDebug && yarn start",
    "apk_release": "cd android && ./gradlew assembleRelease",
    "apk_debug": "cd android && ./gradlew assembleDebug",
    "bundle_release": "cd android && ./gradlew bundleRelease",
    "gradle_clean": "cd android && ./gradlew clean",
    "gradle_build": "cd android && ./gradlew build",
    "reconnect_usbDebug": "adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "stepzen_start": "cd stepzen && stepzen start --dashboard=local"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "graphql": "^16.6.0",
    "react": "18.2.0",
    "react-native": "0.71.2",
    "react-native-maps": "^1.4.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-vector-icons": "^9.2.0",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "postcss": "^8.4.21",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}